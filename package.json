{
  "name": "cf-svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite dev --host",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "cert": "rm -rf .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem localhost 127.0.0.1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "migration:apply": "wrangler d1 migrations apply DB --local",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-09-25 --proxy 5173 -- npm run dev",
    "pages:deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.48.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "@sveltejs/kit": "^2.7.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "^9.6.0",
    "eslint": "^9.0.0",
    "eslint-plugin-jsdoc": "^50.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.5",
    "tslib": "^2.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "type": "module"
}
